		            	out.print(rs.getInt(6));
		            	out.print("</td>");
		            	
		            	out.print("<td>");
		            	out.print(rs.getInt(7));
		            	out.print("</td>");
		            	

		            	out.print("<td>");
		            	out.print(rs.getInt(8));
		            	out.print("</td>");
		            	
		            	out.print("</tr>");
		            	
		            	
		            	
		 
		            }
